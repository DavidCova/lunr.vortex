    "phpmailer/phpmailer": "~6.4",
    "rmccue/requests": "~1.8.0",
    "php": ">=7.4",
    "ext-xml": "*"
  },
  "require-dev": {
    "lunr/halo": "~0.7.0",
    "phpunit/phpunit": ">=8.5 <9.6",
    "ext-xdebug": ">=3.0"
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
        "php": "7.4.0"
    }
  },
  "autoload": {
    "psr-4": {"Lunr\\Vortex\\": "src/Lunr/Vortex/"}
